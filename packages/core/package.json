{
  "name": "@leeburn/v-element",
  "version": "1.0.0",
  "description": "Components library by Vue3 + Ts",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/umd/index.umd.js",
  "module": "./dist/es/index.es.js",
  "types": "./dist/types/core/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/index.es.js",
      "require": "./dist/umd/index.umd.js",
      "types": "./dist/types/core/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-es": "vite build --config build/vite.es.config.ts",
    "build-umd": "vite build --config build/vite.umd.config.ts",
    "build": "pnpm run build-es && pnpm run build-umd"

  },
  "keywords": ["vue3","ts","components","ui"],
  "author": "leeburn",
  "license": "ISC",
  "packageManager": "pnpm@9.6.0",
  "peerDependencies": {
    "vue": "^3.4.27"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.31.0",
    "vite-plugin-compression": "^0.5.1"
  }

}